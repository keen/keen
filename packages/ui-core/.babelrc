{
  "presets": [
    ["@babel/preset-env", { "modules": false, "exclude": ["babel-plugin-transform-async-to-generator", "babel-plugin-transform-regenerator"] }],
  ],
  "plugins": [
    [
      "@quickbaseoss/babel-plugin-styled-components-css-namespace",
      {"cssNamespace": "&&&"}
    ],
    ["babel-plugin-styled-components", {
      "namespace": "keen-ui-core",
      "minify": true,
      "transpileTemplateLiterals": true
    }]
  ],
}
